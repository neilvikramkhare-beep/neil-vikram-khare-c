#include <stdio.h>
int main()
{
    int a[100],count[100]={0};
    int i,j,n;
    printf("enter number of elements:");
    scanf("%d,&n");
    if(n<1)
    return 1;
    if(n>100)
    return 1;
    printf("enter %d positive integers:\n");
    for(i=0;i<n;i++)
{
    scanf("%d,&a[i]");
    if(a[i]<0)
    return 1;
    count[a[i]]=count[a[i]]+1;
    }
    i=0;
    j=0;
    while(j<=100)
    {
        if(count[j]=count[j-1])
        {
            else
            {
                j=j+1;
            }
        }
        printf("sorted array:\n");
        i=0;
        while(i<n)
        {
            printf("%d,a[i]");
            i=i+1;
        }
        printf("\n");
    }
    return 0;
    }
