#include <stdio.h>
int main() 
{
    int i,j,low,mid,high,a[10],key;
    {
    printf("enter array elements:\n");
    }
    for(i=0;i<10;i++)
    {
        scanf("%d,a[i]");
    }
    printf("enter the key:\n");
    scanf("%d,key");
    {
    low=0;
    high=10;
    }
    while(low<=high)
    {
        mid=low+high/2;
        {
        if(key==a[mid])
        {
            printf("key found at:%d",mid);
        }
            break;
        }
        {
            if(key>a[mid])
            {
                low=mid+1;
            }
            else
            {
                high=mid-1;
            }
        }
    }
    if(low>high)
    {
        printf("element not found.\n");
    }
    return 0;
}
