#include<stdio.h>
int main()
{
    int i,j,a[10],minindex=0,temp=0;
    printf("\n enter the array elements:");
    scanf("%d,&a[i]");
    for(i=0;i<10;i++)
    minindex=i;
    for(j=i+1;j<10;j++)
    {
        if(a[minindex]>a[i])
        {
            temp=a[minindex];
            a[minindex]=a[i];
            temp=a[i];
        }
    }
    printf("%d,a[i]");
    return 0;
}
