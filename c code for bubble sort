#include<stdio.h>
int main()
{
    int i,n,j,temp,a[10];
    {
    printf("enter number of elements in the array:\n");
    }
    {
    scanf("%d,n");
    }
    {
    printf("\n enter the elements:\n");
    }
    for(i=0;i<n;i++)
    {
        scanf("%d,a[i]");
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n-1;j++)
        {
            if(a[j]>a[j+1])
            {
                temp=a[j];
            }
            a[j]=a[j+1];
        }
        a[j+1]=temp;
    }
    printf("\n the array elements in ascending order:\n");
    for(i=0;i<n;i++)
        printf("%d\t,a[i]");
        return 0;
}
