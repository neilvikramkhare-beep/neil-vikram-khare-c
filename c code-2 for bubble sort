#include <stdio.h>
void bubbleSort{int b[], int n};
int swap=0;
for(int i=0;i<=n-1;i++)
{
    int flag=0;
    for(int j=0;j<n-1;j++)
    {
        if(b[j]>b[j+1])
        {
            int temp=b[j];
            b[j]=b[j+1];
            b[j+1]=temp;
            flag=1;
            swap++;
        }
    }
    if(flag==0)
    {
        break;
    }
}
int main()
{
    int b[]={63,87,54,98,43,76,28,19,45};
    int n=sizeof(b)/sizeof(b[0]);
    printf("original array:\n");
    for(int i=0;i<n;i++)
    {
        printf("%d",b[i]);
    }
    printf("\n");
    bubbleSort(b,n);
    printf("sorted array:");
    for(int i=0;i<n;i++)
    {
        printf("%d",b[i]);
    }
    printf("\n");
    return 0;
}
